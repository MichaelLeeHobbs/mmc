{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "getAttachmentIds": "readonly",
    "msg": "writable",
    "tmp": "writable",
    "message": "writable",
    "response": "writable",
    "responseStatus": "writable",
    "responseErrorMessage": "writable",
    "responseStatusMessage": "writable",
    "connectorMessage": "readonly",
    "XML": "readonly",
    "$co": "readonly",
    "$c": "readonly",
    "$s": "readonly",
    "$gc": "readonly",
    "$g": "readonly",
    "$cfg": "readonly",
    "$r": "readonly",
    "$": "readonly",
    "channelName": "readonly",
    "channelId": "readonly",
    "router": "readonly"
  },
  //  "parser": "@babel/eslint-parser",
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs"],
    "camelcase": 1,
    "computed-property-spacing": [2, "never"],
    "curly": ["error"],
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "max-depth": [1, 3],
    "max-len": [1, 160],
    "max-statements": [1, 30],
    "new-cap": 1,
    "no-extend-native": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "keyword-spacing": [2, {"before": true, "after": true}],
    "space-unary-ops": 2,
    "space-infix-ops": ["error"],
    "template-tag-spacing": ["error", "never"]
  }
}
